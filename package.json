{
  "name": "priconne-helper",
  "version": "1.0.0",
  "description": "Priconne helper",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "npm run serve --prefix client",
    "clientinstall": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-validator": "^6.1.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.4"
  },
  "devDependencies": {
    "concurrently": "^6.0.2",
    "nodemon": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cahuizar/priconne-helper.git"
  },
  "keywords": [
    "priconne"
  ],
  "author": "Carlos Huizar",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cahuizar/priconne-helper/issues"
  },
  "homepage": "https://github.com/cahuizar/priconne-helper#readme"
}
